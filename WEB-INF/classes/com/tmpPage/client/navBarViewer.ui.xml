<!-- 										<b:AnchorListItem icon="LINE_CHART" text="Alert Handling (Sales Manager page)" addStyleNames="{style.dropMenuNavBarItem}"/> -->
<!-- 									</b:DropDownMenu> -->
<!-- 								</b:ListDropDown> -->
<!-- 								<b:AnchorListItem icon="WARNING" addStyleNames="{style.menuItemActive}" text="Alerts"/> -->
							</b:NavbarNav>
							<b:NavbarNav pull="RIGHT">
								<b:AnchorListItem icon="USER" iconPosition="RIGHT" text="User Name" enabled="false"/>
								<b:AnchorListItem icon="SIGN_OUT" iconPosition="RIGHT" text="Sign Out" enabled="false"/>							
							</b:NavbarNav>								
					</b:Column>	
				</b:Row>	
			</b:Navbar>									
		</b:Container>
	</g:ScrollPanel>
	

</ui:UiBinder> 