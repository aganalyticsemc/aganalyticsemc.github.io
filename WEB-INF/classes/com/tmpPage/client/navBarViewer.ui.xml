<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	         xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="styleNavBar.css" />
	<g:ScrollPanel>
		<b:Container fluid="false">
		<b:Navbar position="FIXED_TOP">						
				<b:Row addStyleNames="{style.navBar}">
					<b:Column size="LG_12,MD_12,SM_12,XS_12" addStyleNames="{style.menuItem}">						
							<b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
								<b:NavbarNav>
									<b:AnchorListItem addStyleNames="{style.menuItemActive}" text="Lomax"/>
<!-- 									<b:AnchorListItem addStyleNames="{style.menuItemActive}" text="Department 2"/> -->
<!-- 									<b:AnchorListItem text="Department 3" /> -->
								</b:NavbarNav>								
								<b:NavbarBrand pull="RIGHT">
									ag analytics
								</b:NavbarBrand>						
					</b:Column>						
				</b:Row>
				<b:Row addStyleNames="{style.subNavBar}">
					<b:Column size="LG_12,MD_12" addStyleNames="{style.subMenuItem}">
							<b:NavbarNav>
								<b:AnchorListItem icon="DASHBOARD" text="Overview" enabled="false"/>
								<b:AnchorListItem icon="TROPHY" text="Ranking" enabled="false"/>
								<b:AnchorListItem icon="BAR_CHART" text="Drivers" enabled="false"/>
								<b:AnchorListItem ui:field="alertTab" icon="WARNING" text="Alerts" />
								<b:AnchorListItem ui:field="textAnalyticTab" icon="WECHAT" text="Text Analytics" />
<!-- 								<b:ListDropDown> -->
<!-- 									<b:AnchorButton dataToggle="DROPDOWN" icon="WARNING" text="Alerts" /> -->
<!-- 									<b:DropDownMenu addStyleNames="{style.dropMenuNavBar}"> -->
